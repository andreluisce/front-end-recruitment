{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true

        },
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "env": {
        "es6": true,
        "jest": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-console": "warn",
        "import/newline-after-import": "off",
        "no-unused-vars": 0,
        "max-len": 0,
        "camelcase": 1,
        "no-debugger": "warn",
        "no-duplicate-imports": "warn",
        "jsx-quotes": "warn",
        "no-else-return": "warn",
        "no-extra-parens": "warn",
        "no-extra-semi": "warn",
        "constructor-super": "warn"
    },

    "globals": {
        "document": true,
        "window": true
    }
}